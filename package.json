    "dotenv": "^16.0.0",
    "ethers": "^5.6.1",
    "express": "^4.17.1",
    "graphql": "^16.3.0",
    "graphql-type-json": "^0.3.2",
    "jsonwebtoken": "^8.5.1",
    "keccak256": "^1.0.6",
    "merkletreejs": "^0.2.31",
    "mongoose": "^6.2.8",
    "morgan": "^1.10.0",
